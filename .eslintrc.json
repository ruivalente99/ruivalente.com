{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "space-in-parens": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            2,
            "never"
        ],
        "no-trailing-spaces": [
            2,
            {
                "skipBlankLines": true
            }
        ],
        "no-console": 1
    }
  }